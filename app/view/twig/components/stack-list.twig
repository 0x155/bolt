<ul>
    {% for file in stack()[:context.count] %}
        <li>
            <a href="{{ file.url }}" class="filebrowserCallbackLink">
                {{- file.filename }}
                <small>
                    (
                    {{- file.sizeFormatted(true) }}
                    {%- if file.image %}, {{ file.width }} Ã— {{ file.height }} px{% endif -%}
                    )
                </small>
            </a>
        </li>
    {% endfor %}
</ul>
