<section>
    <h2>Last modified {{contenttype.name}}</h2>
    <ul>
    {% for content in latest %}
        <li>
           â„– {{content.id}}. <a href="{{ path('editcontent', { 'contenttypeslug': contenttype.slug, 'id': content.id }) }}">
            {% if content.title is defined %}
                {{content.title|excerpt(70) }}
            {% else %}
                {{content.name|excerpt(70) }}
            {% endif %}
            </a> <small>({{ content.datechanged|date("Y/m/d H:i") }})</small>
        </li>
    {% else %}
        <li><em>No latest content</em></li>
    {% endfor %}
    </ul>
</section>
    