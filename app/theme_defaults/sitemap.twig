<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
    {% for link in links %}
    <url>
        <loc>{{ app.paths.hosturl }}{{ link.loc }}</loc>
        {% if link.lastmod is defined %}
        <lastmod>{{ link.lastmod }}</lastmod>
        {% endif %}
    </url>
    {% endfor %}
</urlset>