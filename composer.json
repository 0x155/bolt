{
    "name" : "bolt/bolt",
    "description" : "Sophisticated, lightweight & simple CMS",
    "homepage" : "http://bolt.cm",
    "keywords" : [
        "CMS",
        "Silex",
        "PHP",
        "Symfony"
    ],
    "license" : "MIT",
    "require" : {
        "bolt/dumper" : "~2.0",
        "bolt/package-wrapper": "~1.0|~2.0",
        "bolt/pathogen" : "~0.6",
        "bolt/thumbs" : "~2.0",
        "brandonwamboldt/utilphp" : "~1.0",
        "cocur/slugify" : "~1.0",
        "composer/composer" : "dev-master@dev",
        "doctrine/dbal" : "^2.5",
        "erusev/parsedown-extra" : "~0.2",
        "ext-mbstring" : "*",
        "ext-pdo" : "*",
        "filp/whoops" : "~1.1",
        "ircmaxell/random-lib" : "~1.1",
        "league/flysystem" : "^1.0",
        "miljar/php-exif": "~0.4",
        "monolog/monolog" : "~1.12",
        "nesbot/carbon" : "^1.20",
        "PasswordLib/PasswordLib": "^1.0@beta",
        "php" : ">=5.4.16",
        "silex/silex" : "^1.3",
        "silex/web-profiler" : "^1.0",
        "siriusphp/upload" : "~1.2",
        "stecman/symfony-console-completion" : "~0.4",
        "swiftmailer/swiftmailer" : "~5.3",
        "symfony/config" : "^2.8@beta",
        "symfony/console" : "^2.8@beta",
        "symfony/debug": "^2.8@beta",
        "symfony/event-dispatcher": "^2.8@beta",
        "symfony/filesystem" : "^2.8@beta",
        "symfony/finder" : "^2.8@beta",
        "symfony/form" : "^2.8@beta",
        "symfony/http-foundation": "^2.8@beta",
        "symfony/http-kernel": "^2.8@beta",
        "symfony/intl" : "^2.8@beta",
        "symfony/options-resolver": "^2.8@beta",
        "symfony/process": "^2.8@beta",
        "symfony/property-access": "^2.8@beta",
        "symfony/routing": "^2.8@beta",
        "symfony/security-core": "^2.8@beta",
        "symfony/security-csrf" : "^2.8@beta",
        "symfony/stopwatch": "^2.8@beta",
        "symfony/translation" : "^2.8@beta",
        "symfony/twig-bridge" : "^2.8@beta",
        "symfony/validator" : "^2.8@beta",
        "symfony/var-dumper" : "^2.8@beta",
        "symfony/web-profiler-bundle" : "^2.8@beta",
        "symfony/yaml" : "^2.8@beta",
        "tdammers/htmlmaid" : "~0.7",
        "twig/twig" : "^1.18",
        "ua-parser/uap-php" : "~3.4"
    },
    "require-dev" : {
        "bolt/codingstyle" : "~1.0",
        "codeception/codeception" : "^2.1",
        "lstrojny/phpunit-function-mocker" : "dev-master@dev",
        "phpmd/phpmd" : "~2.2",
        "phpunit/dbunit" : "~1.3",
        "phpunit/php-code-coverage" : "~2.0",
        "phpunit/phpunit" : "~4.4",
        "sebastian/phpcpd" : "~2.0",
        "squizlabs/php_codesniffer" : "~2.0",
        "symfony/phpunit-bridge" : "^2.8@beta"
    },
    "config" : {
        "preferred-install" : "dist"
    },
    "autoload" : {
        "psr-4" : {
            "Bolt\\" : "src"
        },
        "files" : [
            "app/deprecated.php"
        ]
    },
    "autoload-dev" : {
        "psr-4" : {
            "Bolt\\Tests\\" : "tests/phpunit/unit"
        }
    },
    "bin" : [
        "app/nut"
    ],
    "extra" : {
        "branch-alias" : {
            "dev-master" : "2.3.x-dev"
        }
    }
}
